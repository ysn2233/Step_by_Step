# set up demo environment
# terminal font size: 16
source ../demo/bin/activate

# demo1: function templates
python processor.py -i -l demo_inputs/draw_square.py

# demo2: code reorganisation
python reorganiser.py -d demo_inputs/simplified_draw_house.py

# demo3: code statistics
python processor.py -s demo_inputs/draw_house.py

# demo4: picture capturing
python capturer.py demo_inputs/draw_house.py

# demo5: turinglab website
# http://app.turinglab.co.uk
# stepbystep
# password

# quit demo environment
deactivate
